# n8n Environment Configuration for AI Video Agent
# Copy this file and update with your actual values

# Payload CMS Configuration
PAYLOAD_CMS_URL=http://localhost:3001
PAYLOAD_API_KEY=automation-users API-Key YOUR_GENERATED_KEY_HERE

# AI Configuration (for enhanced analysis)
OPENAI_API_KEY=sk-your-openai-api-key-here

# YouTube API (if you want direct YouTube integration in n8n)
YOUTUBE_API_KEY=your-youtube-api-key-here

# n8n Configuration
N8N_HOST=0.0.0.0
N8N_PORT=5678
N8N_PROTOCOL=http
WEBHOOK_URL=http://localhost:5678/

# Database (if using n8n with database persistence)
# DB_TYPE=postgresdb
# DB_POSTGRESDB_HOST=localhost
# DB_POSTGRESDB_PORT=5432
# DB_POSTGRESDB_DATABASE=n8n
# DB_POSTGRESDB_USER=n8n
# DB_POSTGRESDB_PASSWORD=your_password

# Security (for production)
# N8N_BASIC_AUTH_ACTIVE=true
# N8N_BASIC_AUTH_USER=admin
# N8N_BASIC_AUTH_PASSWORD=secure_password

# Advanced n8n Settings
N8N_DEFAULT_BINARY_DATA_MODE=filesystem
N8N_BINARY_DATA_TTL=60
N8N_LOG_LEVEL=info

# Optional: Custom timeout for long-running operations
EXECUTIONS_TIMEOUT=900
EXECUTIONS_TIMEOUT_MAX=3600
